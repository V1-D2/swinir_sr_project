{
    "name": "temperature_swinir_bsrgan_x4_2nd_generation",
    "model_type": "SwinIR",
    "scale": 4,
    "gpu_ids": [0],

    "network_g": {
        "type": "SwinIR",
        "upscale": 4,
        "in_chans": 1,
        "img_size": 128,
        "window_size": 8,
        "img_range": 1.0,
        "depths": [6, 6, 6, 6, 6, 6],
        "embed_dim": 180,
        "num_heads": [6, 6, 6, 6, 6, 6],
        "mlp_ratio": 4,
        "upsampler": "nearest+conv",
        "resi_connection": "3conv"
    },

    "datasets": {
        "train": {
            "name": "TemperatureDataset",
            "data_type": "temperature",
            "dataroot": "./data",
            "scale": 4,
            "patch_size": 128,
            "use_bsrgan": true,
            "batch_size": 8,
            "num_workers": 4,
            "pin_memory": true
        },
        "val": {
            "name": "TemperatureDataset",
            "data_type": "temperature",
            "dataroot": "./data",
            "scale": 4,
            "patch_size": 128
        }
    },

    "train": {
        "optim_g": {
            "type": "Adam",
            "lr": 3e-4,
            "weight_decay": 1e-4,
            "betas": [0.9, 0.99]
        },
        "scheduler": {
            "type": "CosineAnnealingLR",
            "T_max": 100,
            "eta_min": 1e-6
        },
        "total_epochs": 100,
        "warmup_epochs": -1,
        "pixel_criterion": "l1",
        "pixel_weight": 1.0,
        "val_freq": 5000,
        "save_checkpoint_freq": 10,
        "print_freq": 100,
        "save_img": true
    },

    "logger": {
        "print_freq": 1000,
        "save_checkpoint_freq": 500,
        "use_tensorboard": true
    }
}